{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/components/Navbar.jsx", "src/components/SearchPage.jsx", "src/components/SearchButton.jsx", "src/components/Logo.jsx", "src/components/Hero.jsx", "src/components/Footer.jsx", "src/components/BlurNav.jsx", "src/components/ArrowDown.jsx"],
  "exclude": ["node_modules"]
}
